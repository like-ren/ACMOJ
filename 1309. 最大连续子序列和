#include <iostream>
using namespace std;

//O(n)时间复杂度的最大连续子序列和求解
//方法：从头遍历，若sum小于零则这一部分必定不包含在最大连续子序列中。此时重置begin
int MaxSubArray(int array[],int length);

int main()
{
    int l,Maxsum;
    cin>>l;
    int *arr=new int[l+1];
    for(int i=0;i<l;i++){
        cin>>arr[i];
    }
    Maxsum=MaxSubArray(arr,l);
    cout<<Maxsum<<endl;
    delete []arr;
    system("pause");
    return 0;
}

int MaxSubArray(int array[],int length){
    int MaxSum=0,NowSum=0;
    for(int i=0;i<length;i++){
        NowSum+=array[i];
        if(NowSum<=0) {NowSum=0;}
        else {
            if(NowSum>MaxSum) MaxSum=NowSum;
        }
    }
    return MaxSum;
}
